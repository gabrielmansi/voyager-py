{
  "name": "sampletask",
  "runner": "python",
  "security": ["_ADMIN"],
  "categories": ["dev"],
  "params": [
	{
		"type": "CatalogPath",
		"name": "sample_catalog",
		"required": true,
		"value": "C:\\voyager\\voyager.shp"
	},
	{
		"type": "CheckBox",
		"name": "sample_checkbox",
		"required": false,
		"value": true	
	},
	{
		"type": "Date",
		"name": "sample_date",
		"required": true
	},
	{
		"type": "DateTime",
		"name": "sample_date_time",
		"required": true
	},
  	{
		"type": "VoyagerResults",
		"name": "sample_results",
		"required": true
  	},
  	{
		"type": "FieldList",
		"name": "sample_field_list",
		"required": true
  	},
  	{
		"type": "Field",
		"name": "sample_field",
		"required": true
  	},
	{
		"type": "FolderLocation",
		"name": "sample_folder",
		"required": true,
		"value": "C:\\voyager\\"
	},
	{
		"type": "Geometry",
		"name": "sample_geometry",
		"required": false,
		"extentParam": false,
		"initWithResultsExtent": false
	},
	{
		"type": "Integer",
		"name": "sample_integer",
		"required": true,
		"min": 1,
		"max": 100,
		"value": 55
	},
	{
		"type": "MapView",
		"name": "sample_mapview",
		"required": false,
		"initWithResultsExtent": false
	},
	{
		"type": "Password",
		"name": "sample_password",
		"required": true,
	},
	{
		"type": "Projection",
		"name": "sample_projection",
		"required": false,
		"code": "Same As Input"
	},
	{
		"type": "StringChoiceFromService",
		"name": "sample_choice_from_service",
		"required": true,
		"url": "http://host/path/to/something",
		"value": "Value Choice 01"
	},
	{
		"type": "StringChoice",
		"name": "sample_choice",
		"required": true,
		"choices": [ "Value Choice 01", "Value Choice 02", "Value Choice 03" ],
		"value": "Value Choice 01"
	},
	{
		"type": "String",
		"name": "sample_string",
		"required": true,
		"value": "value of sample_string"
	},
	{
		"type": "TextArea",
		"name": "sample_text_area",
		"required": true,
		"value": "value of sample_string"
	},
  ],
  "display": {
  	"en": {
  		"display": "Sample Task",
  		"description": "A pretend/synthesized example that uses all Voyager task parameter types for testing/training.",
  		"params": {
  			"sample_string": {"display": "Sample String", "description": "An example of a String parameter type ('string')."},
  			"sample_integer": {"display": "Sample Integer", "description": "An example of an Integer parameter type (99)."},
  			"sample_choice": {"display": "Sample StringChoice", "description": "An example of a Choice parameter type ('string')."},
  			"sample_catalog": {"display": "Sample CatalogPath", "description": "An example of a Catalog parameter type ('c:\\string\\path\\to\\file.shp')."},
  			"sample_folder": {"display": "Sample FolderLocation", "description": "An example of a FolderLocation parameter type ('c:\\string\\path\\to\\folder\\')."},
  			"sample_checkbox": {"display": "Sample CheckBox", "description": "An example of a CheckBox parameter type (true/false)."},
  			"sample_projection": {"display": "Sample Projection", "description": "An example of a Projection parameter type (4326)."},
  			"sample_geometry": {"display": "Sample Geometry", "description": "An example of a Geometry parameter type ('BBOX/WKT')."},
  			"sample_mapview": {"display": "Sample MapView", "description": "An example of a MapView parameter type ('BBOX')."}
  		}
  	}
  }
}
